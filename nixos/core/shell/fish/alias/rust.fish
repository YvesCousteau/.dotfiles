alias clippy-full='cargo clippy --all-targets --all-features -- -D warnings -W clippy::all -W clippy::pedantic -W clippy::nursery -W clippy::perf -W clippy::complexity -W clippy::suspicious -W clippy::style -W clippy::correctness'
alias clippy-all='cargo clippy --all-targets --all-features -- -D warnings -W clippy::all'
alias clippy-cargo='cargo clippy --all-targets --all-features -- -D warnings -W clippy::cargo'
alias clippy-nursery='cargo clippy --all-targets --all-features -- -D warnings -W clippy::nursery'
alias clippy-pedantic='cargo clippy --all-targets --all-features -- -D warnings -W clippy::pedantic'
alias clippy-perf='cargo clippy --all-targets --all-features -- -D warnings -W clippy::perf'
alias clippy-restriction='cargo clippy --all-targets --all-features -- -D warnings -W clippy::restriction'
alias clippy-complexity='cargo clippy --all-targets --all-features -- -D warnings -W clippy::complexity'
alias clippy-style='cargo clippy --all-targets --all-features -- -D warnings -W clippy::style'
alias clippy-suspicious='cargo clippy --all-targets --all-features -- -D warnings -W clippy::suspicious'
alias clippy-correctness='cargo clippy --all-targets --all-features -- -D warnings -W clippy::correctness'
alias cargo-check-udeps='rustup run nightly cargo udeps --all-targets  --all-features'
alias cargo-tree-search='cargo tree -i'
